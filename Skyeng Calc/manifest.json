{
	"manifest_version":2,
	"name": "Tech Calc Skyeng",
	"description": "1st line support calculator",
	"version": "1.3.6",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "calc.png",
		"48": "calc.png",
		"128": "calc.png"
	},
	"permissions": [
		  "storage"
        ],
	"background": {
		"scripts": [
			"background.js"
		]
    },
	"content_scripts": [
		{
        "js": ["content-script.js"],
		"matches": ["https://help.skyeng.ru/staff/cases/record/*"]
		},
		{
			"js": ["content-script.js"],
			"matches": ["https://skyeng.omnidesk.ru/staff/cases/record/*"]
		}
	],
    "web_accessible_resources": ["script.js"]
}