{
    "name": "Script Package",
    "version": "0.3.4",
    "description": "Сборник всех скриптов для сотрудников Skyeng. По проблемам обратитесь в канал Slack: #tech-extensions",
    "manifest_version": 2,
    "icons": {
		"16": "script-pack.png",
		"48": "script-pack.png",
		"128": "script-pack.png"
	},
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "content_scripts": [
    {
        "js": ["timetable.js"],
        "matches": ["*://timetable.skyeng.ru/*"]
    },
    {
        "js": ["ticket_quote.js", "ticket_how_old_ticket.js", "ticket_how_old_reply.js", "ticket_big_notes.js", "ticket_input_label.js", "ticket_easy_info_window.js", "ticket_reminds_of.js", "ticket_who_creator.js", "ticket_dark_css.js", "ticket_help_notify.js"],
        "matches": ["*://help.skyeng.ru/staff/cases/record/*","*://skyeng.omnidesk.ru/staff/cases/record/*"]
    },
    {
        "js": ["info_user_status.js", "info_user_button.js"],
        "matches": ["*://id.skyeng.ru/admin/users/*"]
    },
    {
        "js": ["crm_teacher_info.js"],
        "matches": ["*://crm.skyeng.ru/admin/orderPriority/search?*"]
    },
    {
        "js": ["chat_separate.js", "chat_crit.js", "chat_close.js", "ticket_easy_info_window.js","chat_new_ticket.js", "ticket_who_creator.js"],
        "matches": ["*://skyeng.omnidesk.ru/staff/cases/chat/*", "*://help.skyeng.ru/staff/cases/chat/*"]
    },
    {
        "js": ["box_ticket_info.js", "box_sort.js", "box_show_all.js","box_in_new_tab.js","box_in_new_tab_checkbox.js"],
        "matches": ["*://help.skyeng.ru/staff/cases/list/*"]
    },
    {
        "js": ["calc_date_text.js","calc_no_back_reload.js","calc_download_events.js","calc_backup.js"],
        "matches": ["*://qie.glitch.me/*"]
    },
    {
        "js": ["workbook.js"],
        "matches": ["*://*.skyeng.ru/workbook/*"]
    },
    {
        "js": ["crm_group.js"],
        "matches": ["*://*.skyeng.ru/admin/group/edit?id=*"]
    },
    {
        "js": ["chat_new_ticket.js"],
        "matches": ["*://skyeng.omnidesk.ru/l_rus/*","*://*.skyeng.ru/l_rus/*"]
    },
    {
        "js": ["ticket_easy_info_window.js"],
        "matches": ["*://skyeng.autofaq.ai/tickets/assigned/*"]
    }],
    "permissions": [
        "*://*.skyeng.ru/*","*://skyeng.omnidesk.ru/*","*://datsy.ru/api/*","storage"
    ]
}